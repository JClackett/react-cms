{
	"name": "cms",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@reach/router": "^1.2.1",
		"antd": "^3.10.8",
		"apollo": "^2.1.3",
		"apollo-boost": "^0.1.22",
		"graphql": "^14.0.2",
		"jest-dom": "^2.1.1",
		"react": "^16.7.0-alpha.2",
		"react-apollo": "^2.3.1",
		"react-apollo-hooks": "^0.1.6",
		"react-dom": "^16.7.0-alpha.2",
		"react-scripts": "^2.1.1",
		"react-testing-library": "^5.3.0",
		"styled-components": "^4.1.1",
		"tslint": "^5.11.0",
		"typescript": "^3.1.6"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"schema:download": "apollo schema:download --endpoint=http://localhost:5000/graphql",
		"codegen:generate": "apollo client:codegen --includes=src/graphql/**/*.tsx --localSchemaFile=schema.json --outputFlat --target=typescript src/graphql/types.tsx",
		"gen:types": "npm run schema:download && npm run codegen:generate"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": [
		">0.2%",
		"not dead",
		"not ie <= 11",
		"not op_mini all"
	],
	"devDependencies": {
		"@types/graphql": "^14.0.3",
		"@types/jest": "^23.3.9",
		"@types/node": "^10.12.10",
		"@types/reach__router": "^1.2.2",
		"@types/react": "^16.7.7",
		"@types/react-dom": "^16.0.10",
		"@types/styled-components": "^4.1.1",
		"tslint": "^5.11.0"
	}
}
